{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2016",
        "declaration": false,
        "module": "commonjs",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": [        
        "./**/*.ts",
        "./**/*.tsx",
        "./parcel.d.ts"       
      ],
      "exclude": [
        "./node_modules",
        "./dist"
      ]
}